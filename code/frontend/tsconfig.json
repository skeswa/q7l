{
  "compilerOptions": {
    "baseUrl": "./src",
    "lib": ["dom", "es2015"],
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "allowSyntheticDefaultImports": true
  },
  "include": ["./src/**/*"]
}
